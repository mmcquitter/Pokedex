    "from bs4 import BeautifulSoup\n",
    "import requests\n",
    "\n",
    "url = 'https://www.pokemon.com/us/pokedex/6'\n",
    "\n",
    "r = requests.get(url).text\n",
    "\n",
    "soup = BeautifulSoup(r, 'lxml')\n",
    "\n",
    "name = str(soup.title.string).split()[0]\n",
    "\n",
    "name"
